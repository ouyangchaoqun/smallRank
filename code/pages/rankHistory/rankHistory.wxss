page{
    height:100%;
    background: pink;
}
.history_top{
    height:106rpx;
    padding-top: 78rpx;
    background: #FD674C;
    position: relative;
    z-index: 1;
}

.cover{ background: #FD674C; height: 14rpx; padding-top: 78rpx; z-index: 2; position: absolute; top:0; width: 100%}
.cover .cover_bar{
    margin:0 auto;
    height:14rpx;
    width:88%;
    border-radius: 13rpx;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
    background: linear-gradient(#922B02, #bf3200);
    background: -webkit-linear-gradient(#922B02, #bf3200);
}


.history_header{
    margin:0 auto;
    margin-top: 7px;
    height:13rpx;
    width:88%;
    border-radius: 13rpx;

    border-top-left-radius: 0;
    border-top-right-radius: 0;
    background: linear-gradient(#bf3200, #D94139);
    background: -webkit-linear-gradient(#bf3200, #D94139);
    position: relative;
}
.history_box{
    position: absolute;
    top:-500rpx;
    width:98%;
    left:1%;
}
.history_item{
    height:88rpx;
    line-height: 88rpx;
    background: linear-gradient(to right ,#FF8A83, #FABD5D);
    background: -webkit-linear-gradient(to right ,#FF8A83, #FABD5D);
    border-radius: 10rpx;
    margin-bottom: 16rpx;
    padding:35rpx 30rpx;
    color:#FFFFFF;
}
.history_item .item_img,.groupImg{
    width:80rpx;
    height:80rpx;
    border: 4rpx solid #fff;
    border-radius: 10rpx;
    float: left;
    margin-right: 16rpx;
    overflow: hidden;
}
.item_img image{
    width:100%;
    height:100%;
}
.groupImg image{ border-radius: 0; margin-right: 0; line-height: 1; display: block; float:left;}
.groupImg.imgG9 image,.groupImg.imgG8 image,.groupImg.imgG7 image,.groupImg.imgG6 image,.groupImg.imgG5 image{ width: 27%; height: 27%; margin-left: 4rpx; margin-top: 4rpx; }
.groupImg.imgG8 image:nth-child(1){ margin-left: 16rpx;}
.groupImg.imgG7 image:nth-child(1){ margin-left: 29rpx; margin-right: 30rpx;}
.groupImg.imgG6 image:nth-child(1),.groupImg.imgG6 image:nth-child(2),.groupImg.imgG6 image:nth-child(3){ margin-top: 16rpx;}
.groupImg.imgG5 image:nth-child(1),.groupImg.imgG5 image:nth-child(2){ margin-top: 16rpx; margin-left: 12rpx;}

.groupImg.imgG4 image,.groupImg.imgG3 image,.groupImg.imgG2 image{ width: 43.5%; height: 43.5%; margin-left: 4rpx; margin-top: 4rpx; }

.groupImg.imgG3 image:nth-child(1){ margin-left: 23rpx;}
.groupImg.imgG2 image:nth-child(1),.groupImg.imgG2 image:nth-child(2){ margin-top: 23rpx;}
.history_item .item_name{
    font-size: 30rpx;
    float: left;
    width:50%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.history_item .item_right{
    float: right;
    font-size: 36rpx;
}


.crile{ height: 200rpx; width:200rpx; border-radius: 50%; background: rgba(255,255,255,.5); position: absolute;

    animation: cirle 16s linear infinite ;
    animation-direction:alternate;
    -webkit-animation-direction:alternate; /* Safari 和 Chrome */
}
.crile2{

    height: 180rpx; width:180rpx; border-radius: 50%; background: rgba(255,255,255,.5); position: absolute;
    animation: cirle2 18s linear infinite ;
    animation-direction:alternate;
    -webkit-animation-direction:alternate; /* Safari 和 Chrome */}
@keyframes cirle {
    0%{transform: translate3d(0, 0, 0)
    }
    20%{transform: translate3d(550rpx, 100rpx, 0)
    }
    30%{transform: translate3d(0, 200rpx, 0)
    }
    50%{transform: translate3d(550rpx, 300rpx, 0)
    }
    60%{transform: translate3d(0, 400rpx, 0)
    }
    70%{transform: translate3d(550rpx, 500rpx, 0)
    }
    90%{transform: translate3d(0, 600rpx, 0)
    }
    100%{transform: translate3d(550rpx, 700rpx, 0)
    }
}
@keyframes cirle2 {
    100%{transform: translate3d(570rpx, 20rpx, 0)
    }
    90%{transform: translate3d(0rpx, 120rpx, 0)
    }
    70%{transform: translate3d(570rpx, 220rpx, 0)
    }
    60%{transform: translate3d(0rpx, 320rpx, 0)
    }
    50%{transform: translate3d(570rpx, 420rpx, 0)
    }
    30%{transform: translate3d(0rpx, 520rpx, 0)
    }
    20%{transform: translate3d(570rpx, 620rpx, 0)
    }
    0%{transform: translate3d(0rpx, 720rpx, 0)
    }
}